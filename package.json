{
  "name": "express5-typescript-template",
  "version": "0.0.0",
  "scripts": {
    "build": "npx ts-node ./scripts/build.ts",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "dev": "dotenvx run -f ./env/.env.development -- npx ts-node ./src",
    "dev:hot": "dotenvx run -f ./env/.env.development -- nodemon --exec \"ts-node --swc ./src\" --watch ./src -e ts",
    "lint": "npx eslint ./src/**/*.ts",
    "lint:tests": "npx eslint ./spec/**/*.ts",
    "start": "dotenvx run -f ./env/.env.production -- node -r module-alias/register ./dist",
    "test": "dotenvx run -f ./env/.env.test -- npx ts-node ./spec",
    "test:hot": "nodemon --exec \"npm run test\" --watch ./src --watch ./spec -e ts"
  },
  "_moduleAliases": {
    "@src": "./dist"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.14.2",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.13",
    "express": "^5.0.0",
    "helmet": "^8.0.0",
    "inserturlparams": "^2.0.4",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.0.9",
    "jsonfile": "^6.1.0",
    "model-initializer": "^1.3.4",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@stylistic/eslint-plugin-ts": "^2.8.0",
    "@swc/core": "^1.7.26",
    "@types/cookie-parser": "^1.4.7",
    "@types/eslint__js": "^8.42.3",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.4",
    "@types/jsonfile": "^6.1.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.4",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.11.1",
    "eslint-plugin-n": "^17.10.3",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "jasmine": "^5.3.1",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  }
}
