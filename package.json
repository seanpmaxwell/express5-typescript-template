{
  "name": "express5-typescript-template",
  "version": "0.0.0",
  "scripts": {
    "build": "tsx ./scripts/build.ts",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "dev": "tsx --env-file=./config/.env.development ./src",
    "dev:hot": "tsx --env-file=./config/.env.development --watch ./src",
    "lint": "eslint ./src",
    "lint:tests": "eslint ./spec",
    "start": "node --env-file=./config/.env.production -r module-alias/register ./dist",
    "test": "tsx --env-file=./config/.env.test ./spec",
    "test:hot": "tsx --env-file=./config/.env.test --watch ./spec --include ./src",
    "type-check": "tsc --noEmit"
  },
  "_moduleAliases": {
    "@src": "./dist"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.13",
    "express": "^5.0.1",
    "helmet": "^8.0.0",
    "inserturlparams": "^2.0.4",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.0.9",
    "jsonfile": "^6.1.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@stylistic/eslint-plugin-ts": "^2.8.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/eslint__js": "^8.42.3",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.4",
    "@types/jsonfile": "^6.1.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.4",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.11.1",
    "eslint-plugin-n": "^17.10.3",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "jasmine": "^5.3.1",
    "supertest": "^7.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  }
}
